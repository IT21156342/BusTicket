<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A54848"
    android:backgroundTint="@color/white"
    tools:context=".UserProfile">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView4"

        android:layout_width="405dp"
        android:layout_height="336dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="543dp"
            android:layout_height="315dp"
            android:background="@color/teal_200">

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="148dp"
                android:text="Edit Profile"
                android:textSize="26dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.153" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="247dp"
                android:layout_height="196dp"
                android:layout_marginTop="100dp"
                android:src="@drawable/avatar_profile_vector_png_clipart"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.283"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/materialCardView3"

        android:layout_width="394dp"
        android:layout_height="399dp"
        android:layout_marginTop="372dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="341dp"

            android:paddingTop="10sp">

            <EditText
                android:id="@+id/date"
                android:layout_width="237dp"
                android:layout_height="48dp"
                android:layout_marginEnd="4dp"
                android:background="@drawable/textshap"
                android:ems="10"
                android:hint="Date of Birth"
                android:inputType="datetime"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.569" />

            <EditText
                android:id="@+id/district"
                android:layout_width="237dp"
                android:layout_height="48dp"
                android:layout_marginEnd="4dp"
                android:background="@drawable/textshap"
                android:ems="10"
                android:hint="Your District"
                android:inputType="textPersonName"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.747" />

            <Button
                android:id="@+id/add_Btn"
                android:layout_width="179dp"
                android:layout_height="54dp"
                android:backgroundTint="@color/purple_700"
                android:text="Add Details"
                app:cornerRadius="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.06"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.996" />

            <Button
                android:id="@+id/editBtn"
                android:layout_width="179dp"
                android:layout_height="54dp"
                android:backgroundTint="@color/teal_700"
                android:text="Edit details"
                app:cornerRadius="24dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.977"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.996" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="123dp"
                android:layout_height="34dp"
                android:layout_marginEnd="8dp"
                android:text="District"
                android:textAlignment="center"
                android:textSize="20sp"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/district"
                app:layout_constraintHorizontal_bias="0.598"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.723" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="123dp"
                android:layout_height="34dp"
                android:text="Date of birth"
                android:textAlignment="center"
                android:textSize="20sp"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/date"
                app:layout_constraintHorizontal_bias="0.444"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.557" />

            <EditText
                android:id="@+id/uType"
                android:layout_width="237dp"
                android:layout_height="48dp"
                android:layout_marginEnd="4dp"

                android:background="@drawable/textshap"
                android:ems="10"
                android:hint="User Type"
                android:inputType="textPersonName"
                android:textAlignment="center"
                android:textIsSelectable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.385" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="123dp"
                android:layout_height="34dp"
                android:layout_marginStart="16dp"
                android:text="User Type"
                android:textAlignment="center"

                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.394" />

            <EditText
                android:id="@+id/mobile"
                android:layout_width="237dp"
                android:layout_height="48dp"
                android:layout_marginEnd="4dp"
                android:background="@drawable/textshap"
                android:ems="10"
                android:hint="Your Mobile Number"
                android:inputType="phone"

                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.221" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="123dp"
                android:layout_height="34dp"
                android:layout_marginStart="16dp"
                android:text="Mobile "
                android:textAlignment="center"

                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.221" />

            <EditText
                android:id="@+id/name"
                android:layout_width="237dp"
                android:layout_height="48dp"
                android:layout_marginEnd="4dp"
                android:background="@drawable/textshap"
                android:ems="10"
                android:hint="Your Name"

                android:inputType="textPersonName"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.054" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="123dp"
                android:layout_height="34dp"
                android:text="User Name"
                android:textAlignment="center"
                android:textSize="20sp"

                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/name"
                app:layout_constraintHorizontal_bias="0.511"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.081" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
</androidx.constraintlayout.widget.ConstraintLayout>